{
    "name": "wiese/approximate-datetime",
    "description": "Handle dates that are not precisely defined",
    "type": "library",
    "require": {
        "php": "^7.1",
        "ext-calendar": "*",
        "monolog/monolog": "^1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "squizlabs/php_codesniffer": "@stable"
    },
    "autoload": {
        "psr-4": {
            "wiese\\ApproximateDateTime\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "wiese\\ApproximateDateTime\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "minimum-stability": "dev"
}
